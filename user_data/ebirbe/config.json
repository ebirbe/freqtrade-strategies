        "pair_whitelist": [
	    // My 7 Pairs
            "BTC/USDT",
            "DASH/USDT",
            "EOS/USDT",
            "ETH/USDT",
            "LRC/USDT",
            "LTC/USDT",
            "YFI/USDT",

	    // Optional Pairs
